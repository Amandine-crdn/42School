#include <stdlib.h>

void	ft_swap(int *a, int *b)
{
	int i;

	i = *a;
	*a = *b;
	*b = i;
}

void	ft_sort_int_tab(int *tab, int size)
{
	int j;
	int i;

	i = 0;
	while (i < size)
	{
		j = i + 1;
		while (j < size)
		{
			if (tab[i] > tab[j])
			{
				ft_swap(&tab[i], &tab[j]);
			}
			j++;
		}
		i++;
	}
}

char	*ft_push_swap(int argc, char *argv[])
{
	int	*stackA;
	int	*stackB;
	int	**chunk;
	int	M;
	int	*sort;
//	stackB = ft_strdup("");
	stackA = (int *)malloc(sizeof(int) * (argc + 1));
	while (argv)
	{
		stackA[n] = argv;
		n++;
	}
	sort = stackA;
	ft_sort_int_tab(sort, argc);
	if (argc % 2 == 0)
		M = (argc)/2;
	else
		M = (argc + 1)/2;
	int n = 0;
	
	//ft_create_chunk
	while (n < M)
	{
		if (argc % 2 == 0)
		{
			if (stackA[0] <= chunk[0][M])
			{
				pb;	
				n++;
			}
			else
				ra;
		}
		else
		{
			if (stackA[0] < chunk[0][M])
			{
				pb;
				n++;
			}
			else
				ra;
		}
	}

	pb




}

